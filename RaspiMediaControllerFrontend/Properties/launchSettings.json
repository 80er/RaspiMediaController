{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iis": {
      "applicationUrl": "http://localhost/RaspiMediaControllerFrontend",
      "sslPort": 0
    },
    "iisExpress": {
      "applicationUrl": "http://localhost:61813/",
      "sslPort": 0
    }
  },
  "profiles": {
    "Electron.NET App": {
      "commandName": "Executable",
      "executablePath": "dotnet.exe",
      "commandLineArgs": "electronize start",
      "workingDirectory": "$(OutputPath)"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "RaspiMediaControllerFrontend": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:61814/"
    }
  }
}